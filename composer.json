{
    "name": "jonpemby/phlash",
    "description": "Lodash inspired helper functions for PHP",
    "type": "library",
    "require-dev": {
        "phpunit/phpunit": "^7.3"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Jonathon Pemberton",
            "email": "jonpemby@icloud.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {},
    "autoload": {
        "psr-4": {
            "Phlash\\": "src/Phlash"
        },
        "files": [
            "src/functions/autoload.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Phlash\\Tests\\": "test/Phlash"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --bootstrap vendor/autoload.php test/Phlash"
    }
}
